    "@types/serviceworker": "^0.0.127",
    "@vite-pwa/sveltekit": "^0.6.7",
    "@vitest/coverage-istanbul": "3.0.9",
    "eslint": "^9.23.0",
    "eslint-plugin-svelte": "^2.46.1",
    "flowbite-svelte": "^0.48.4",
    "globals": "^16.0.0",
    "jsdom": "^26.0.0",
    "marked": "^15.0.7",
    "msw": "^2.7.3",
    "playwright": "^1.51.1",
    "prettier": "^3.5.3",
