    "@types/serviceworker": "0.0.138",
    "@vite-pwa/sveltekit": "^1.0.0",
    "@vitest/coverage-istanbul": "3.2.1",
    "eslint": "9.28.0",
    "eslint-plugin-svelte": "^3.9.1",
    "flowbite-svelte": "1.5.3",
    "globals": "16.2.0",
    "jsdom": "26.1.0",
    "marked": "15.0.12",
    "msw": "2.7.3",
    "playwright": "1.52.0",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "3.4.0",
    "sass-embedded": "1.89.1",
    "svelte": "5.33.14",
    "svelte-check": "4.2.1",
    "typescript": "5.8.3",
    "typescript-eslint": "8.33.1",
