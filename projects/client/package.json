    "@ts-rest/core": "^3.52.1",
    "@use-gesture/vanilla": "^10.3.1",
    "date-fns": "^4.1.0",
    "firebase": "^11.8.1",
    "idb-keyval": "^6.2.2",
    "resolve-accept-language": "^3.1.11",
    "workbox-expiration": "^7.3.0",
    "workbox-precaching": "^7.3.0",
