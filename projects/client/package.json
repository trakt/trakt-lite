{
  "name": "client",
  "version": "0.0.1",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build:preview": "IS_PREVIEW=true vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "test:unit": "vitest",
    "test": "deno task test:unit --run && deno task test:e2e",
    "test:e2e": " PW_DISABLE_TS_ESM=true playwright test",
    "i18n:resolve": "deno run --allow-read --allow-write .scripts/resolve-i18n.ts",
    "i18n:traktify": "deno run --allow-read --allow-write --allow-net --allow-env .scripts/traktify-i18n.ts",
    "i18n:delete": "deno run --allow-read --allow-write --allow-env .scripts/delete-i18n.ts"
  },
  "devDependencies": {
    "@google/generative-ai": "^0.21.0",
    "@playwright/test": "^1.49.0",
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/adapter-cloudflare": "^4.7.4",
    "@sveltejs/enhanced-img": "^0.4.1",
    "@sveltejs/kit": "^2.8.5",
    "@sveltejs/vite-plugin-svelte": "^5.0.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.6",
    "@testing-library/user-event": "^14.5.2",
    "@types/eslint": "^9.6.1",
    "eslint": "^9.15.0",
    "eslint-plugin-svelte": "^2.46.0",
    "globals": "^15.12.0",
    "jsdom": "^25.0.1",
    "msw": "^2.6.8",
    "prettier": "^3.4.1",
    "prettier-plugin-svelte": "^3.3.2",
    "sass-embedded": "^1.83.0",
    "svelte": "^5.2.9",
    "svelte-check": "^4.1.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0",
    "vite": "^6.0.1",
    "vitest": "^2.1.6"
  },
  "dependencies": {
    "@inlang/paraglide-sveltekit": "^0.12.1",
    "@tanstack/svelte-query": "^5.61.5",
    "@tanstack/svelte-query-devtools": "^5.61.5",
    "@ts-rest/core": "^3.51.0",
    "date-fns": "^4.1.0",
    "firebase": "^11.0.2",
    "zod": "^3.23.8"
  }
}
