{
  "name": "client",
  "version": "0.0.1",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "dev:staging": "IS_STAGING=true vite dev",
    "dev:contrib": "IS_CONTRIB=true vite dev",
    "build": "vite build",
    "build:preview": "IS_PREVIEW=true vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "test:unit": "vitest",
    "test:cov": "vitest --coverage",
    "test:e2e": "cucumber-js -c ./cucumber.mjs",
    "i18n:resolve": "deno run --allow-read --allow-write .scripts/resolve-i18n.ts",
    "i18n:traktify": "deno run --allow-read --allow-write --allow-net --allow-env .scripts/traktify-i18n.ts",
    "i18n:cleanse": "deno run --allow-read --allow-write --allow-env .scripts/cleanse-i18n.ts"
  },
  "devDependencies": {
    "@cucumber/cucumber": "11.3.0",
    "@google/generative-ai": "0.24.1",
