{
  "$schema": "../schema/meta-messages.schema.json",
  "meta": {
    "locale": "nl-nl",
    "direction": "ltr",
    "guidance": "Use Dutch with appreciation for both international cinema and Dutch productions. Say 'film' for movies, 'serie' for shows. Keep it straightforward but enthusiastic - Dutch audiences love quality storytelling and aren't afraid to binge-watch!",
    "generator": {
      "inlang": {
        "enabled": true,
        "outputPath": "./messages/{locale}.json"
      },
      "android": {
        "enabled": true,
        "outputPath": "./android/values-{locale}/strings.xml",
        "resourceName": "strings"
      },
      "ios": {
        "enabled": true,
        "outputPath": "./ios/Localizable.xcstrings"
      }
    }
  },
